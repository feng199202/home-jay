<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tarena.netctoss.entity">

	<class name="UserService" table="t_user_service">
		<id name="serviceId" column="service_id">
			<generator class="native" />
		</id>
		<many-to-one name="user" column="user_id"></many-to-one>
		<property name="loginName" column="login_name" length="20"></property>
		<property name="loginPassword" column="login_password"
			length="20"></property>
		<property name="labIp" column="lab_ip" length="20"></property>
		<property name="serviceStatus" column="service_status"></property>
		<many-to-one name="pricing" column="pricing_id" />
		<property name="enrollDate" column="enroll_date" type="date"></property>
		<property name="closeDate" column="close_date" type="date"></property>

	</class>

</hibernate-mapping>  