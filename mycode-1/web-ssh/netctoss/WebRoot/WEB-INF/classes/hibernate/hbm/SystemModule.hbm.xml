<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tarena.netctoss.entity">

	<class name="SystemModule" table="t_system_module">
	
		<id name="systemModuleId" column="system_module_id">
			<generator class="native"/>
		</id>
		<property name="systemModuleName" column="system_module_name" length="50"></property>
		<property name="url" length="100"></property>
		<set name="rights" inverse="true" lazy="false" >
			<key>
				<column name="system_module_id"></column>
			</key>
			<one-to-many class="com.tarena.netctoss.entity.Right"/>
		</set>
	</class>

</hibernate-mapping>  