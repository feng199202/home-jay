<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tarena.netctoss.entity">

	<class name="Admin" table="t_admin">
		<id name="adminId" column="admin_id">
			<generator class="native" />
		</id>
		<property name="realName" column="real_name" length="20" />
		<property name="accountName" column="account_name" length="20" />
		<property name="password" column="password" length="20" />
		<property name="adminPhone" column="admin_phone" length="20" />
		<property name="adminEmail" column="admin_email" length="30" />
		<property name="enrollDate" column="enroll_date" type="date"/>
		<many-to-one name="role" column="role_id" />
	</class>
</hibernate-mapping>    