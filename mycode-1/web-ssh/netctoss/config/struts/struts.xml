<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
	<!-- 项目公共信息 -->
	<constant name="struts.i18n.encoding" value="UTF-8" />
	<constant name="struts.serve.static.browserCache" value="false" />
	<constant name="struts.devMode" value="true" />
	<constant name="struts.i18n.reload" value="true" />
	<constant name="struts.configuration.xml.reload" value="true" />
	<constant name="struts.ui.theme" value="simple" />
	<constant name="struts.objectFactory" value="spring" />
	<package name="netctoss-default" extends="struts-default"
		namespace="/">
		<!-- <default-action-ref name="userAction" /> -->
		<global-results>
			<result name="error">/error.jsp</result>
		</global-results>
		<!-- <action name="userAction" class="userAction"> <result name="success" 
			type="dispatcher"> /WEB-INF/jsp/index.jsp </result> </action> -->
	</package>
	<include file="struts/struts-admin.xml"></include>
	<include file="struts/struts-user.xml"></include>
	<include file="struts/struts-pricing.xml"></include>
	<include file="struts/struts-manage-user.xml"></include>
</struts>
