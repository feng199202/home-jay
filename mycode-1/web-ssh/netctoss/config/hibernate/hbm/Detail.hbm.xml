<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tarena.netctoss.entity">

	<class name="Detail" table="t_detail">
		<id name="detailId" column="detail_id">
			<generator class="native"></generator>
		</id>
		<property name="loginName" column="login_name" length="20"></property>
		<property name="loginIp" column="login_ip" length="24"></property>
		<property name="loginDate" column="login_date" type="timestamp"></property>
		<property name="logoutDate" column="logout_date" type="timestamp"></property>
		<property name="labIp" column="lab_ip" length="24"></property>
		<property name="duration"></property>
		<property name="serviceName" column="service_name"></property>
		<property name="fee"></property>
		<property name="monthCurrentFee" column="month_current_fee"></property>
		<property name="userName" column="user_name"></property>
		<property name="year"></property>
		<property name="month"></property>
		<property name="day"></property>
	</class>

</hibernate-mapping>  